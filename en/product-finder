    $(function () {
        var $btn = $('#btnTop');
        var $home = $('#topSection');
        var start = 800;
        $(window).on('scroll', function () {
            if ($(window).scrollTop() > start) {
                $btn.show();
            } else {
                $btn.hide();
            }
        });

