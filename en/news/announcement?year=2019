            <script>
        $('.news-items').equalize({
            children: '.news-info'
        });

        $(document).click(function(e) {
            if (!$("#new-cate").is(e.target) && !$("#new-cate").has(e.target).length) {
                $('#new-cate').removeClass('show');
            }
            if (!$("#new-year").is(e.target) && !$("#new-year").has(e.target).length) {
                $('#new-year').removeClass('show');
            }
        });
    </script>
