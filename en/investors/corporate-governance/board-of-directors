    <script>
        $(".nav-tabs a").click(function () {
            $(".nav-link").removeClass("active");
            $(this).tab('show');
        });

        $('.tabs-slick').slick({
            infinite: false,
            slidesToShow: 2,
            slidesToScroll: 1,
            mobileFirst: true,
            responsive: [{
                breakpoint: 767,
                settings: "unslick"
            }]
        });

        //select Tabs
        $("#majorSelect").change(function () {
            var year = $(this).val();
            if (!year) return;

            $("#majorResolutions .articleItem").hide()
                .filter('[data-year=' + year + ']').fadeIn();
        }).trigger('change');

        $("#diversitySelect").change(function () {
            var year = $(this).val();
            if (!year) return;

            $("#diversityPolicy .policyItem").hide()
                .filter('[data-year=' + year + ']').fadeIn();
        }).trigger('change');
    </script>
