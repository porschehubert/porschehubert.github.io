<script>
    $(function () {
        var $btn = $('#btnTop');
        var $home = $('#topSection');
        var start = 800;
        $(window).on('scroll', function () {
            if ($(window).scrollTop() > start) {
                $btn.show();
            } else {
                $btn.hide();
            }
        });

        // Animation
        new WOW().init();
    });
</script>
<script>
    $(function () {
        $.cookieBar(
            'addTranslation', 'tw', {
                message: '我們使用cookie來提供我們的服務。 使用本網站即表示您同意。',
                acceptText: '同意',
                infoText: '更多資訊',
                privacyText: '隱私保護'
            }
        );

