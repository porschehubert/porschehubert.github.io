    <script>
        $('.tabs-slick').slick({
            infinite: false,
            slidesToShow: 7,
            slidesToScroll: 1,
            adaptiveHeight: true,
            responsive: [{
                breakpoint: 991,
                settings: {
                    slidesToShow: 4,
                    slidesToScroll: 1,
                }
            },{
                breakpoint: 768,
                settings: {
                    slidesToShow: 2,
                    slidesToScroll: 1,
                }
            }]
        });

        $(".slick-slide.slick-current").addClass("active");

        $(".nav-tabs a").click(function() {
            $(".nav-link").removeClass("active");
            $(".slick-slide").removeClass("active");
            $(this).closest('.slick-active').addClass('active');
            $(this).tab('show');
        });

        // Subject Select
        var filterActive;
        function filterCategory(category) {
            if (filterActive != category) {
                $('.filter-cat-results .f-cat').removeClass('active');
                $('.filter-cat-results .f-cat')
                    .filter('[data-cat="' + category + '"]')
                    .addClass('active');
                filterActive = category;
            }
        }
        $('.filtering select').change(function() {
            filterCategory($(this).find('option:selected').data('filter'));
        });

                $("select[name='subject']").val('products');
        filterCategory('cat-sales');
            </script>
